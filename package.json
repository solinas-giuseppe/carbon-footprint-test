{
	"name": "carbon-footprint-test",
	"description": "a website that tells you the carbon impact of your website",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro check && astro build",
		"preview": "astro preview",
		"astro": "astro",
		"test:astro:watch": "vitest --config vitest.config.astro.ts",
		"test:svelte:watch": "vitest --config vitest.config.svelte.ts --disable-console-intercept ",
		"test:astro:run": "vitest run --config vitest.config.astro.ts",
		"test:svelte:run": "vitest run --config vitest.config.svelte.ts",
		"sst:deploy": "pnpm test:astro:run && pnpm test:svelte:run && pnpm test:e2e && sst deploy",
		"sst:dev": "sst dev",
		"cy:run": "cypress run --e2e",
		"test:e2e": "start-server-and-test \"pnpm dev --port 3000\" http-get://localhost:3000/en/ \"cypress run --e2e\""
	},
	"dependencies": {
		"@astrojs/check": "^0.7.0",
		"@astrojs/mdx": "^3.1.0",
		"@astrojs/node": "^8.2.5",
		"@astrojs/svelte": "^5.4.0",
		"@astrojs/tailwind": "^5.1.0",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"astro": "^4.9.2",
		"astro-seo": "^0.8.4",
		"svelte": "^4.2.17",
		"svelte-collapse": "^0.1.2",
		"tailwindcss": "^3.4.3",
		"typescript": "^5.4.5"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^6.4.6",
		"@testing-library/svelte": "^5.1.0",
		"astro-sst": "^2.43.0",
		"aws-cdk-lib": "2.142.1",
		"constructs": "10.3.0",
		"cypress": "^13.11.0",
		"jsdom": "^24.1.0",
		"msw": "^2.3.1",
		"prettier": "^3.3.1",
		"prettier-plugin-astro": "^0.14.0",
		"prettier-plugin-svelte": "^3.2.3",
		"prettier-plugin-tailwindcss": "^0.6.1",
		"sst": "^2.43.0",
		"start-server-and-test": "^2.0.4",
		"vite-tsconfig-paths": "^4.3.2",
		"vitest": "^1.6.0"
	}
}
